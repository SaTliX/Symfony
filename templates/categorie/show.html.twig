                <h3 class="pb-4 mb-4 fst-italic border-bottom">{{ categorie.name }}</h3>
                <div id="cours-list">
                    {% include 'cours/list.html.twig' with { cours: categorie.cours , leftCol: 5 , rightCol: 7 }%}
                </div>
            </div>
        </div>
    </main>
{% endblock %}