{% extends 'base.html.twig' %}

{% block title %}StudentAnswer index{% endblock %}

{% block body %}
    <h1>StudentAnswer index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Result</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for student_answer in student_answers %}
            <tr>
                <td>{{ student_answer.id }}</td>
                <td>{{ student_answer.Result }}</td>
                <td>
                    <a href="{{ path('app_student_answer_show', {'id': student_answer.id}) }}">show</a>
                    <a href="{{ path('app_student_answer_edit', {'id': student_answer.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_student_answer_new') }}">Create new</a>
{% endblock %}
